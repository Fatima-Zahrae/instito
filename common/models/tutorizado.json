{
  "name": "Tutorizado",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "verificado": {
      "type": "boolean"
    },
    "verificationToken": {
      "type": "string",
      "length": 512
    }
  },
  "validations": [],
  "relations": {
    "tutorado": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": "tutorado"
    },
    "tutor": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": "tutor"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
